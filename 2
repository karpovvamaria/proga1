#include <stdio.h>

// Функція для знаходження найбільшого спільного дільника
int ohd(int a, int b) {
    if (b == 0)
        return a;
    return ohd(b, a % b);
}

// Функція для знаходження найменшого спільного кратного
int lom(int a, int b) {
    return (a * b) / ohd(a, b);
}

int main() {
    int n;
    printf("Введіть кількість чисел: ");
    scanf("%d", &n);

    int numbers[20];
    printf("Введіть %d чисел, розділених пробілом: ", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &numbers[i]);
    }

    int result = numbers[0];
    for (int i = 1; i < n; i++) {
        result = lom(result, numbers[i]);
    }

    printf("Найменше спільне кратне: %d\n", result);

    return 0;
}
